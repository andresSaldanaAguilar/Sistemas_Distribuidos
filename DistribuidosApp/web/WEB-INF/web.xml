<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletRegistrar</servlet-name>
        <servlet-class>ServletRegistrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroServlet</servlet-name>
        <servlet-class>Servlets.RegistroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaServlet</servlet-name>
        <servlet-class>Servlets.ConsultaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CargaServlet</servlet-name>
        <servlet-class>Servlets.CargaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletRegistrar</servlet-name>
        <url-pattern>/ServletRegistrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroServlet</servlet-name>
        <url-pattern>/RegistroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaServlet</servlet-name>
        <url-pattern>/ConsultaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CargaServlet</servlet-name>
        <url-pattern>/CargaServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
